@model IEnumerable<Albergo.Models.Camera>

@{
    ViewBag.Title = "Camera";
}

<h2>Camera</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdCamera)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Numero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.IdCliente)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdCamera)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numero)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.IdCliente)
        </td>
        
    </tr>
}

</table>
